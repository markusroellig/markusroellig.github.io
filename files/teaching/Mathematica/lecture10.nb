Cell[TextData[{
 "We want to find the circle, such that the sum of the squares of the \
distances of the points to this circle becomes minimal. The function ",
 StyleBox["distance", "MR"],
 " measures the distance between a circle and a point "
}], "Text",
 CellChangeTimes->{3.4698117040719004`*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"distance", "[", 
   RowBox[{
    RowBox[{"Circle", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"mpx_", ",", " ", "mpy_"}], "}"}], ",", " ", "r_"}], "]"}], 
    ",", " ", 
    RowBox[{"point", ":", 
     RowBox[{"{", 
      RowBox[{"x_", ",", " ", "y_"}], "}"}]}]}], "]"}], " ", ":=", "\n", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"R", " ", "=", " ", 
      RowBox[{"Sqrt", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"mpx", " ", "-", " ", "x"}], ")"}], "^", "2"}], " ", "+", 
        " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"mpy", " ", "-", " ", "y"}], ")"}], "^", "2"}]}], "]"}]}], 
     "}"}], ",", " ", 
    RowBox[{"Sqrt", "[", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"r", " ", "-", " ", "R"}], ")"}], "^", "2"}], "]"}]}], "]"}], 
  " "}]], "Input"],

Cell["These data yield the following function to be minimized.", "Text",
 CellChangeTimes->{3.4698117486919627`*^9}],

Cell[BoxData[
 RowBox[{
  RowBox[{"toMinimize", " ", "=", " ", 
   RowBox[{"Plus", " ", "@@", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"distance", "[", 
         RowBox[{
          RowBox[{"Circle", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"mpx", ",", " ", "mpy"}], "}"}], ",", " ", "r"}], "]"}], 
          ",", " ", "#"}], "]"}], "^", "2"}], "&"}], " ", "/@", " ", 
      "circleData"}], ")"}]}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"#", ",", " ", 
     RowBox[{"Timing", "[", 
      RowBox[{"FindMinimum", "[", 
       RowBox[{"toMinimize", ",", " ", 
        RowBox[{"{", 
         RowBox[{"mpx", ",", " ", "1"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"mpy", ",", " ", "1"}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"r", ",", " ", "2"}], "}"}], ",", "\n", 
        "                   ", 
        RowBox[{"Method", " ", "->", " ", "#"}]}], "]"}], "]"}]}], "}"}], 
   "&"}], " ", "/@", " ", "\n", "       ", 
  RowBox[{"Append", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "\"\<Gradient\>\"", ",", " ", "\"\<Newton\>\"", ",", " ", 
      "\"\<QuasiNewton\>\"", ",", " ", "\"\<ConjugateGradient\>\""}], "}"}], 
    ",", "\n", "              ", "\"\<LevenbergMarquardt\>\""}], 
   "]"}]}]], "Input"],

