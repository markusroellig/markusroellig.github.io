Cell[TextData[{
 Cell[BoxData[
  RowBox[{
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", "x"}], "]"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"\[PartialD]", 
      RowBox[{"f", "[", "x", "]"}]}], 
     RowBox[{"\[PartialD]", "x"}]], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"f", "'"}], "[", "x", "]"}], "=", 
     RowBox[{
      RowBox[{
       SuperscriptBox["f", "\[Prime]"], "[", "x", "]"}], "=", 
      RowBox[{
       SubscriptBox["\[PartialD]", "x"], 
       RowBox[{"f", "[", "x", "]"}]}]}]}]}]}]], "InlineInput",
  Evaluatable->False,
  CellChangeTimes->{{3.4639876644142*^9, 3.4639876695992002`*^9}}],
 "\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "n"}], "}"}]}], "]"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[PartialD]", "n"], 
      RowBox[{"f", "[", "x", "]"}]}], 
     RowBox[{"\[PartialD]", 
      SuperscriptBox["x", "n"]}]], "=", 
    RowBox[{
     SubscriptBox["\[PartialD]", 
      RowBox[{"{", 
       RowBox[{"x", ",", "n"}], "}"}]], 
     RowBox[{"f", "[", "x", "]"}]}]}]}]], "InlineInput",
  Evaluatable->False],
 "\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"f", "[", 
      RowBox[{"x", ",", "y"}], "]"}], ",", "x", ",", "y"}], "]"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[PartialD]", "2"], 
      RowBox[{"f", "[", 
       RowBox[{"x", ",", "y"}], "]"}]}], 
