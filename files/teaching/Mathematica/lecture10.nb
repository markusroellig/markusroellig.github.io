 Cell[BoxData[
  FormBox[
   RowBox[{"f", 
    RowBox[{"(", "0", ")"}], "+", 
    SuperscriptBox["f", "\[Prime]"], 
    RowBox[{"(", "0", ")"}], "x", "+", 
    SuperscriptBox["f", "\[Prime]\[Prime]"], 
    RowBox[{"(", "0", ")"}], 
    SuperscriptBox["x", "2"], "/", "2", "+", "\[Ellipsis]", 
    SuperscriptBox["f", 
     RowBox[{"(", "n", ")"}]], 
    RowBox[{"(", "0", ")"}], 
    SuperscriptBox["x", "n"], "/", "n", "!"}], TraditionalForm]], 
  "InlineMath"],
