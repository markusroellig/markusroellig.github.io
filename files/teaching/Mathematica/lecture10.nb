Cell[CellGroupData[{

Cell["Example: Minimal sum of squares", "Subsubsection",
 CellChangeTimes->{{3.469811603401759*^9, 3.4698116585818367`*^9}}],

Cell["Take 1000 points scattered around a circle.", "Text",
 CellChangeTimes->{{3.469811671841855*^9, 3.469811674601859*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"SeedRandom", "[", "999", "]"}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{"\n", 
  RowBox[{
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Delta]", " ", "=", " ", "0.1"}], "}"}], ",", "\n", 
     RowBox[{"circleData", " ", "=", " ", 
      RowBox[{"Table", "[", "\n", "  ", 
       RowBox[{
        RowBox[{
         RowBox[{"Function", "[", 
          RowBox[{"\[CurlyPhi]", ",", " ", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Cos", "[", "\[CurlyPhi]", "]"}], ",", " ", 
              RowBox[{"Sin", "[", "\[CurlyPhi]", "]"}]}], "}"}], " ", "+", 
            " ", "\n", "            ", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Random", "[", 
               RowBox[{"Real", ",", " ", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"-", "\[Delta]"}], ",", " ", "\[Delta]"}], "}"}]}], 
               "]"}], ",", " ", 
              RowBox[{"Random", "[", 
               RowBox[{"Real", ",", " ", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"-", "\[Delta]"}], ",", " ", "\[Delta]"}], "}"}]}], 
               "]"}]}], "}"}]}]}], "]"}], "[", "\n", 
         "                           ", 
         RowBox[{"Random", "[", 
          RowBox[{"Real", ",", " ", 
           RowBox[{"{", 
            RowBox[{"0", ",", " ", 
