Cell[TextData[{
 " The discrete Fourier transform is implemented in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " as follows. ",
 Cell[BoxData[
  RowBox[{
   ButtonBox["Fourier",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/Fourier"], "[", 
   StyleBox["list", "TI"], "]"}]], "InlineFormula"],
 " finds the discrete Fourier transform of a list of complex numbers. ",
 "Multidimensional data can be transformed by \n",
 StyleBox["Fourier[{{", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "11"], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "12"], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox["\[Ellipsis]", TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"1", " ", "n"}]], TraditionalForm]], "InlineFormula"],
 StyleBox["}, \n         {", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "21"], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "22"], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox["\[Ellipsis]", TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    RowBox[{"2", " ", "n"}]], TraditionalForm]], "InlineFormula"],
 StyleBox["}, ", "MR"],
 Cell[BoxData[
  FormBox["\[Ellipsis]", TraditionalForm]], "InlineFormula"],
 StyleBox[", \n         {", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    StyleBox[
     RowBox[{
      StyleBox["m", "TI"], "1"}]]], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    StyleBox[
     RowBox[{
      StyleBox["m", "TI"], "2"}]]], TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox["\[Ellipsis]", TraditionalForm]], "InlineFormula"],
 StyleBox[", ", "MR"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", 
    StyleBox["\"\<mn\>\"", "TI"]], TraditionalForm]], "InlineFormula"],
 StyleBox["}}]", "MR"],
 ". \nIf we input exact data, the data will be numericalized to machine \
precision. "
}], "Text",
 CellChangeTimes->{{3.4697968180653305`*^9, 3.46979683931536*^9}, 
   3.46979692314948*^9}],

Cell[TextData[{
 "This means that for a vector ",
 Cell[BoxData[
  FormBox[
   StyleBox["l",
    FontWeight->"Bold"], TraditionalForm]]],
 " of length ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]]],
 ", ",
 StyleBox["Fourier[", "MR"],
 StyleBox["l", "TI",
  FontWeight->"Bold"],
 StyleBox["]", "MR"],
 " effectively carries out the matrix product ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["F",
     FontWeight->"Bold",
     FontSlant->"Plain"], ".", 
    StyleBox["l",
     FontWeight->"Bold"]}], TraditionalForm]]],
 ", where the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"j", ",", "k"}], ")"}], TraditionalForm]]],
 " element of the square matrix ",
 Cell[BoxData[
  FormBox[
   StyleBox["F",
    FontWeight->"Bold",
    FontSlant->"Plain"], TraditionalForm]]],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["n", 
     RowBox[{
      RowBox[{"-", "1"}], "/", "2"}]], 
    RowBox[{"exp", "(", 
     RowBox[{"2", "\[Pi]", " ", 
      RowBox[{"i", "(", 
       RowBox[{"k", "-", "1"}], ")"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"j", "-", "1"}], ")"}], "/", "n"}]}], ")"}]}], 
   TraditionalForm]]],
 "."
}], "Text",
 CellChangeTimes->{3.469796952391522*^9}],

