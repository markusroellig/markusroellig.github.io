Cell[BoxData[
 RowBox[{"NIntegrate", "[", 
  RowBox[{
   RowBox[{"Sin", "[", 
    RowBox[{"Sin", "[", "x", "]"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", "2"}], "}"}]}], "]"}]], "Input"],

Cell["\<\
Compute a multidimensional integral (with singularity at the origin):\
\>", "Text",
 CellChangeTimes->{3.4639970110629997`*^9}],

Cell[BoxData[
 RowBox[{"NIntegrate", "[", 
  RowBox[{
   FractionBox["1", 
    SqrtBox[
     RowBox[{
      FractionBox["x1", "3"], "+", 
      FractionBox["x2", "2"], "+", 
      FractionBox["x3", "2"], "+", 
      FractionBox["x4", "10"]}]]], ",", 
   RowBox[{"{", 
    RowBox[{"x1", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x2", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x3", ",", "0", ",", "1"}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x4", ",", "0", ",", "1"}], "}"}]}], "]"}]], "Input"],

Cell[TextData[{
 "Sometimes, ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " will not be able to find results for symbolic interals, or it will take a \
very long time. \n",
 Cell[BoxData[
  RowBox[{
   ButtonBox["NIntegrate",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/NIntegrate"], "[", 
   RowBox[{
    StyleBox["f", "TI"], ",", 
    RowBox[{"{", 
     RowBox[{
      StyleBox["x", "TI"], ",", 
      SubscriptBox[
       StyleBox["x", "TI"], 
       StyleBox["min", "TI"]], ",", 
      SubscriptBox[
       StyleBox["x", "TI"], 
       StyleBox["max", "TI"]]}], "}"}]}], "]"}]], "InlineFormula"],
 " ",
 "gives a numerical approximation to the integral ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Integral]", 
     SubscriptBox["x", 
      StyleBox["min",
       FontSlant->"Italic"]], 
     SubscriptBox["x", 
      StyleBox["max",
       FontSlant->"Italic"]]], " ", 
    RowBox[{"f", " ", "d", "\[InvisibleSpace]", "x"}]}], TraditionalForm]], 
  "InlineMath"],
 ". "
}], "Text",
 CellChangeTimes->{{3.4639959793328*^9, 3.4639960112438*^9}, {
  3.4639960427248*^9, 3.4639960483648*^9}}],

Cell["Check the computation Time for the following integral:", "Text",
 CellChangeTimes->{{3.4639960559368*^9, 3.4639960683118*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Integrate", "[", 
   RowBox[{
