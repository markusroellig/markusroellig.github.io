          RowBox[{"(", 
           RowBox[{"-", "1"}], ")"}], "^", "j"}], " ", "j"}], ",", " ", 
        "True", ",", " ", "0"}], "]"}], ",", "\n", "         ", 
      RowBox[{"{", 
       RowBox[{"i", ",", " ", "400"}], "}"}], ",", " ", 
      RowBox[{"{", 
