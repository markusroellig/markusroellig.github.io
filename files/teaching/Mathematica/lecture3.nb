Cell[TextData[{
 ButtonBox["/.",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/ReplaceAll"],
 " is the short form of ",
 ButtonBox["ReplaceAll",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/ReplaceAll"]
}], "Text",
 CellChangeTimes->{{3.4653064782074003`*^9, 3.465306501567*^9}}],

Cell[TextData[{
 "A ",
 ButtonBox["Rule",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/Rule"],
 " is represented by ",
 StyleBox["-> or \[Rule]. ", "OperatorCharacter"],
 Cell[BoxData[
  RowBox[{
   StyleBox["lhs", "TI"], "->", 
   StyleBox["rhs", "TI"]}]], "InlineFormula"],
 " or ",
 Cell[BoxData[
  RowBox[{
   StyleBox["lhs", "TI"], "\[Rule]", 
   StyleBox["rhs", "TI"]}]], "InlineFormula"],
 " represents a rule that transforms ",
 Cell[BoxData[
  StyleBox["lhs", "TI"]], "InlineFormula"],
 " to ",
 Cell[BoxData[
  StyleBox["rhs", "TI"]], "InlineFormula"],
 ". The character ",
 Cell[BoxData["->"], "InlineFormula"],
 " can be entered as ",
 StyleBox["Esc", "KeyEvent"],
 StyleBox["\[ThinSpace]->\[ThinSpace]", "InlineCode"],
 StyleBox["Esc", "KeyEvent"],
 " or ",
 ButtonBox["\[Backslash][Rule]",
  BaseStyle->"Link",
  ButtonData->"paclet:ref/character/Rule"],
 ".",
 Cell[BoxData[
  RowBox[{
   StyleBox[" ", "TI"], 
   RowBox[{
    StyleBox["lhs", "TI"], "->", 
    StyleBox["rhs", "TI"]}]}]], "InlineFormula"],
 " evaluates ",
 Cell[BoxData[
  StyleBox["rhs", "TI"]], "InlineFormula"],
 " immediately. "
}], "Text",
 CellChangeTimes->{{3.4653065313786*^9, 3.4653065582418003`*^9}, {
  3.4653066283014*^9, 3.4653066751949997`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"x", "+", "y"}], "/.", 
