  RowBox[{"integrate", "[", 
   RowBox[{
    RowBox[{"x_", "^", "n_."}], ",", "x_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"x", "^", 
     RowBox[{"(", 
      RowBox[{"n", "+", "1"}], ")"}]}], "/", 
    RowBox[{"(", 
     RowBox[{"n", "+", "1"}], ")"}]}], "/;", 
