Cell["\<\
Now we want to try and model the emission of such a gas. We need to know the \
optical depth of a given column of gas with temperature T. 
\[Tau]=\[Integral]n(l)\[Kappa](l)\[DifferentialD]l,  \[Kappa], the absorption \
coefficient of transition i\[Rule]i-1 depends on the population of level i \
and i-1.\
\>", "Text",
 CellChangeTimes->{{3.4667908980421286`*^9, 3.4667909123005543`*^9}, {
  3.4667917354672*^9, 3.4667918786130514`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Kappa]Rot", "[", 
    RowBox[{"T_", ",", "B_", ",", "jUp_", ",", "A_"}], "]"}], ":=", 
   RowBox[{
    FractionBox[
     SuperscriptBox["cl", "2"], 
     RowBox[{" ", 
      RowBox[{"8", "\[Pi]", " ", 
       SuperscriptBox[
        RowBox[{"\[CapitalDelta]\[Nu]", "[", 
         RowBox[{"jUp", ",", "B"}], "]"}], "2"]}]}]], 
    RowBox[{"aJ", "[", 
     RowBox[{"jUp", ",", "A"}], "]"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"g", "[", "jUp", "]"}], 
        RowBox[{"g", "[", 
         RowBox[{"jUp", "-", "1"}], "]"}]], 
       RowBox[{"population", "[", 
        RowBox[{
         RowBox[{"jUp", "-", "1"}], ",", "T", ",", "B"}], "]"}]}], "-", 
      RowBox[{"population", "[", 
       RowBox[{"jUp", ",", "T", ",", "B"}], "]"}]}], ")"}], " ", 
    FractionBox["cl", 
     RowBox[{
      RowBox[{"\[CapitalDelta]\[Nu]", "[", 
       RowBox[{"jUp", ",", "B"}], "]"}], " ", 
      SuperscriptBox["10", "5"]}]]}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Tau]Rot", "[", 
    RowBox[{
    "T_", ",", "B_", ",", "jUp_", ",", "l_", ",", "\[CapitalDelta]v_", ",", 
     "nCO_", ",", "A_"}], "]"}], ":=", 
   RowBox[{
    SubsuperscriptBox["\[Integral]", "0", "l"], 
    RowBox[{
     FractionBox[
      RowBox[{"nCO", " ", 
       RowBox[{"\[Kappa]Rot", "[", 
        RowBox[{"T", ",", "B", ",", "jUp", ",", "A"}], "]"}]}], 
      "\[CapitalDelta]v"], 
     RowBox[{"\[DifferentialD]", "x"}]}]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.4667919300931416`*^9, 3.4667919474247723`*^9}}],

Cell["\<\
Now we can calculate the emission in units of main beam temperature:\
\>", "Text",
 CellChangeTimes->{{3.4667919068803005`*^9, 3.4667919151483154`*^9}, {
  3.4667919626659985`*^9, 3.4667919800132294`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"tMainBeamRotLTE", "[", 
    RowBox[{
    "T_", ",", "B_", ",", "jUp_", ",", "l_", ",", "\[CapitalDelta]v_", ",", 
     "nCO_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["h", "k"], "\[CapitalDelta]v", " ", 
    RowBox[{"\[CapitalDelta]\[Nu]", "[", 
     RowBox[{"jUp", ",", "B"}], "]"}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Exp", "[", 
        FractionBox[
         RowBox[{"h", " ", 
          RowBox[{"\[CapitalDelta]\[Nu]", "[", 
           RowBox[{"jUp", ",", "B"}], "]"}]}], 
         RowBox[{"k", " ", "T"}]], "]"}], "-", "1"}], ")"}], 
     RowBox[{"-", "1"}]], 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{"Exp", "[", 
       RowBox[{"-", 
        RowBox[{"\[Tau]Rot", "[", 
         RowBox[{
         "T", ",", "B", ",", "jUp", ",", "l", ",", "\[CapitalDelta]v", ",", 
          "nCO", ",", 
          RowBox[{"aJ", "[", 
           RowBox[{"jUp", ",", "A10"}], "]"}]}], "]"}]}], "]"}]}], ")"}]}]}], 
  ";", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{"K", " ", "km", " ", 
     SuperscriptBox["s", 
      RowBox[{"-", "1"}]]}], " ", ",", " ", 
    RowBox[{
    "\[CapitalDelta]v", " ", "is", " ", "the", " ", "line", " ", "width", " ",
      "in", " ", 
     RowBox[{"km", "/", "s"}]}]}], " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.466791986565241*^9, 3.466791995925257*^9}, {
  3.4667922015960183`*^9, 3.4667922607981224`*^9}}],

