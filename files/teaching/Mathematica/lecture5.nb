Cell[TextData[{
 "Another way to specify a color is ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    ButtonBox["Hue",
     BaseStyle->"Link",
     ButtonData->"paclet:ref/Hue"], "[", 
    StyleBox["h", "TI"], "]"}], ","}]], "InlineFormula"],
 " a graphics directive which specifies that objects which follow are to be \
displayed, if possible, in a color corresponding to hue ",
 Cell[BoxData[
  StyleBox["h", "TI"]], "InlineFormula"],
 ". ",
 Cell[BoxData[
  RowBox[{
   ButtonBox["Hue",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/Hue"], "[", 
   RowBox[{
    StyleBox["h", "TI"], ",", 
    StyleBox["s", "TI"], ",", 
    StyleBox["b", "TI"]}], "]"}]], "InlineFormula"],
 "specifies colors in terms of hue, saturation and brightness. "
}], "Text",
 CellChangeTimes->{{3.4653933333970003`*^9, 3.4653933811954*^9}}],

Cell[BoxData[
 RowBox[{"ListAnimate", "[", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"Graphics", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Hue", "[", "i", "]"}], ",", 
       RowBox[{"Disk", "[", "]"}]}], "}"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"i", ",", "0", ",", "1", ",", "0.05"}], "}"}]}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.4653934139709997`*^9, 3.465393468961*^9}}],

Cell["Here is a rough translation Hue <-> color names:", "Text",
 CellChangeTimes->{{3.46677401565501*^9, 3.4667740420190563`*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
