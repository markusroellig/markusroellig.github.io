     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", "1"}], ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", 
       RowBox[{"-", "1"}]}], "}"}]}], "}"}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"Graphics", "[", 
    RowBox[{"GraphicsComplex", "[", 
     RowBox[{"v", ",", 
      RowBox[{"Polygon", "[", 
       RowBox[{"{", 
        RowBox[{"1", ",", "2", ",", "3", ",", "4"}], "}"}], "]"}]}], "]"}], 
    "]"}], ",", 
   RowBox[{"Graphics", "[", 
    RowBox[{"GraphicsComplex", "[", 
     RowBox[{"v", ",", 
      RowBox[{"Line", "[", 
       RowBox[{"{", 
