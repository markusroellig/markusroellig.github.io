    RowBox[{"p_", ",", " ", 
     RowBox[{"{", 
      RowBox[{"p1_", ",", " ", "p2_"}], "}"}]}], "]"}], " ", ":=", " ", 
   StyleBox[
    RowBox[{"(*", " ", 
     RowBox[{"mirrors", " ", "p", " ", "on", " ", "line", " ", "p1p2"}], " ", 
     "*)"}], "CodeComment"], "\n", "        ", 
   RowBox[{
    RowBox[{"Function", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"d", ",", " ", "e"}], "}"}], ",", " ", 
