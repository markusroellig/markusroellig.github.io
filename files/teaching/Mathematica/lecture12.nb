Cell[TextData[{
 "An equation or system of equations involving only polynomials can be solved \
numerically using ",
 Cell[BoxData[
  FormBox[
   ButtonBox["NSolve",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/NSolve"], TraditionalForm]]],
 StyleBox["[eqs,vars,n]", "InlineInput",
  FontWeight->"Bold"],
 " where ",
 StyleBox["eqs", "InlineInput",
  FontWeight->"Bold"],
 " is an equation or list of equations, ",
 StyleBox["vars", "InlineInput",
  FontWeight->"Bold"],
 " is the variable or list of variables, and ",
 StyleBox["n", "InlineInput"],
 " is an optional parameter specifying the precision sought in terms of \
number of decimal digits.  The result is returned as a list of sets of \
replacement rules."
}], "Text",
 CellChangeTimes->{{3.4639832147644*^9, 3.4639832277564*^9}}],

Cell[BoxData[
 RowBox[{"NSolve", "[", 
  RowBox[{
   RowBox[{
