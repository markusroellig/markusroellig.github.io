Cell[TextData[{
 "Perhaps the most useful, but often the most frustrating, ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " function for manipulation of symbolic expressions is ",
 Cell[BoxData[
  FormBox[
   ButtonBox["Simplify",
    BaseStyle->"Link",
    ButtonData->"paclet:ref/Simplify"], TraditionalForm]]],
 ", which attempts to reduce an expression to a simpler form.  ",
 StyleBox["Simplify", "InlineInput",
  FontWeight->"Bold"],
 " includes expansion, factorization, and many other algebraic \
transformations.  With the option ",
 StyleBox["Trig\[Rule]True", "InlineInput",
  FontWeight->"Bold"],
 ", which is the default, it applies trigonometric identities also.  Thus, \
because ",
 StyleBox["Simplify", "InlineInput",
  FontWeight->"Bold"],
 " includes all of the functions described above, plus others, the first step \
in simplifying an expression is usually to append a simplification command in \
postfix notation, namely ",
 StyleBox["expr//Simplify", "InlineInput",
  FontWeight->"Bold"],
 ", and seeing what you get.  "
}], "Text",
 CellChangeTimes->{{3.463980483962*^9, 3.463980496635*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"(", 
    RowBox[{"4", " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"Cosh", "[", "x", "]"}], "6"], " ", 
        SuperscriptBox[
         RowBox[{"Sinh", "[", "x", "]"}], "2"]}], "+", 
       RowBox[{"2", " ", 
        SuperscriptBox[
         RowBox[{"Cosh", "[", "x", "]"}], "4"], " ", 
        SuperscriptBox[
         RowBox[{"Sinh", "[", "x", "]"}], "4"]}], "+", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"Cosh", "[", "x", "]"}], "2"], " ", 
        SuperscriptBox[
         RowBox[{"Sinh", "[", "x", "]"}], "6"]}]}], ")"}]}], ")"}], "/", 
   RowBox[{"(", 
    RowBox[{"8", "+", 
     RowBox[{"12", " ", "x"}], "+", 
     RowBox[{"6", " ", 
      SuperscriptBox["x", "2"]}], "+", 
     SuperscriptBox["x", "3"]}], ")"}]}], " ", "//", "Simplify"}]], "Input"],

Cell[BoxData[
 RowBox[{"Simplify", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Sin", "[", "x", "]"}], "^", "2"}], "+", 
   RowBox[{
    RowBox[{"Cos", "[", "x", "]"}], "^", "2"}]}], "]"}]], "Input"],

