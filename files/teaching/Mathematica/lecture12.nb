   RowBox[{"3", "/", 
    RowBox[{"(", 
     RowBox[{"x", "+", "3"}], ")"}]}], "+", 
   RowBox[{"x", "/", 
    RowBox[{"(", 
     RowBox[{"x", "+", "3"}], ")"}]}]}], "]"}]], "Input",
 CellChangeTimes->{{3.4639807977623997`*^9, 3.4639808068104*^9}}],

Cell[BoxData[
 RowBox[{"Simplify", "[", 
  RowBox[{
   RowBox[{"(", 
