 RowBox[{
  RowBox[{"LOC", "[", 
   RowBox[{"\[Theta]_Symbol", ",", 
    RowBox[{"a_", "?", "NumericQ"}], ",", 
    RowBox[{"b_", "?", "NumericQ"}], ",", 
    RowBox[{"c_", "?", "NumericQ"}]}], "]"}], ":=", 
  RowBox[{"ArcCos", "[", 
   FractionBox[
    RowBox[{
     SuperscriptBox["a", "2"], "+", 
     SuperscriptBox["b", "2"], "-", 
     SuperscriptBox["c", "2"]}], 
    RowBox[{"2", "a", " ", "b"}]], "]"}]}]], "Input",
